{
  "modo_operacao": "aprendizagem",
  "passos_totais": 150,
  "delay_entre_passos": 0.001,
  "num_episodios": 100,
  "usar_visualizacao": true,

  "ambiente": {
    "tipo": "LABIRINTO",
    "parametros": {
      "largura": 12,
      "altura": 12,
      "densidade_paredes": 0.3,
      "pos_inicio": {"x": 0, "y": 0},
      "pos_fim": {"x": 11, "y": 11}
    }
  },

  "agentes": [
    {
      "id": "genetico_labirinto",
      "tipo": "genetico",
      "posicao_inicial": {"x": 0, "y": 0},
      "parametros": {
        "taxa_mutacao": 0.05,
        "tamanho_populacao": 20,
        "num_geracoes": 30}
    },
    {
      "id": "qlearning_labirinto",
      "tipo": "qlearning",
      "posicao_inicial": {"x": 0, "y": 0},
      "parametros": {
        "alpha": 0.1,
        "gamma": 0.95,
        "epsilon": 1.0,
        "epsilon_decay": 0.995,
        "epsilon_min": 0.01,
        "modo_aprendizagem": true
      }
    }
  ]
}

